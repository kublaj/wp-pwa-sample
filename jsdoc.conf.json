{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
      "include": ["src"]
    },
    "plugins": [
      "plugins/markdown",
      "plugins/summarize"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "template": "templates/default",
      "encoding": "utf8",
      "destination": "doc",
      "recurse": true
    }
}
